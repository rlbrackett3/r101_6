<%= form_for @user do |f| %>
<%=f.label :fname %>
<%=f.text_field :fname %>
<%=f.label :lname %>
<%=f.text_field :lname %>
<%=f.label :username %>
<%=f.text_field :username %>
<%=f.label :email %>
<%=f.text_field :email %>
<%=f.label :password %>
<%=f.text_field :password %>
<%=f.submit @button %>
<%end%>